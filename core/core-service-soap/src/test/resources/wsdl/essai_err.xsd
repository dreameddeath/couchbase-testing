<xs:schema
        targetNamespace="http://www.dreameddeath.com/Interfaces/test/v0/data"
        xmlns="http://www.dreameddeath.com/Interfaces/test/v0/data"
        xmlns:tns="http://www.dreameddeath.com/Interfaces/test/v0/data"
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
    >

    <xs:complexType name="TestFault">
        <xs:sequence>
            <xs:element name="Fault" type="xs:string">
                <xs:annotation>
                    <xs:documentation source="GDM">This is the default structure proposed for fault messages, you must declare a Fault message for each error or warning that can occurs</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:element name="TestFault" type="tns:TestFault" />
</xs:schema>